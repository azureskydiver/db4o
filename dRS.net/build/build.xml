<?xml version="1.0"?>

<project name="dRS.net-dummy" default="publish" xmlns:ivy="antlib:fr.jayasoft.ivy.ant">
	<import file="../../build-1.1/common-1.1.xml"/>
	<target name="configure-ivy" >
        <ivy:configure file="${ivy.conf.dir}/ivyconf.xml" />
		<ivy:resolve file="${dir.dRS.net.build}/ivy.xml"/>
	</target>
	<target name="publish" depends="configure-ivy">
		<ant antfile="${dir.dRS.net.build}/build-dRS.net.xml"
		     dir="${dir.dRS.net.build}"/>
	</target>
</project>