        <pre class="prettyprint lang-vb" xml:space="preserve">SortingExample.vb: GetObjectsNQ
Private Shared Sub GetObjectsNQ()
            Dim db As IObjectContainer = Db4oFactory.OpenFile(Db4oFileName)
            Try
                Dim dt1 As DateTime = DateTime.UtcNow
                Dim result As IObjectSet = db.Query(New AllPilots, _ 
New PilotQueryComparer)
                Dim dt2 As DateTime = DateTime.UtcNow
                Dim diff As TimeSpan = dt2 - dt1
                Console.WriteLine( _ 
"Time to execute with NQ and comparator: " + _ 
diff.TotalMilliseconds.ToString() + " ms.")
                ListResult(result)
            Finally
                db.Close()
            End Try
        End Sub</pre>
