public static void AdvancedQuery(IObjectContainer db)
         {
            IList&lt;Pilot&gt; result = db.Query&lt;Pilot&gt;(delegate(Pilot pilot)
             {
                return pilot.Points &gt; 99
                    &amp;&amp; pilot.Points &lt; 199
                    || pilot.Name == &quot;Rubens Barrichello&quot;;
            });
        }</pre>    </body></html>