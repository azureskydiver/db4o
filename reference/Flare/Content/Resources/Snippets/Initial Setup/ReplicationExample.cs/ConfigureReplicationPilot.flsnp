         {
            IConfiguration db4oConfig = Db4oFactory.NewConfiguration();
            db4oConfig.ObjectClass(typeof(Pilot)).GenerateUUIDs(true);
            db4oConfig.ObjectClass(typeof(Pilot)).GenerateVersionNumbers(true);
