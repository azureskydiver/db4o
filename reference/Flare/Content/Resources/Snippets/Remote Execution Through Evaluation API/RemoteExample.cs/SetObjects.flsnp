private static void SetObjects()
     {
      File.Delete(Db4oFileName);
      IObjectContainer db = Db4oFactory.OpenFile(Db4oFileName);
      try 
       {
        for (int i = 0; i &lt; 5; i++) 
         {
          Car car = new Car(&quot;car&quot;+i);
          db.Store(car);
        }
        db.Store(new RemoteExample());
      } 
      finally 
       {
        db.Close();
      }
      CheckCars();
    }</pre>    </body></html>