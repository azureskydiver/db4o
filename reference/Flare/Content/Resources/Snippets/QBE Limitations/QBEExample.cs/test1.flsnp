        <pre class="prettyprint" xml:space="preserve">QBEExample.cs: test1
private static void Test1()
         {
            IObjectContainer container = Database();
            if (container != null)
             {
                try
                 {
                    // Pilot1 contains re-initialisation in the constructor
                    Pilot1 pilot = new Pilot1("Kimi Raikonnen", 100);
                    container.Set(pilot);
                    container.Commit();
                    // QBE returns result with wrong points
                    IObjectSet result = container.Get(
new Pilot1("Kimi Raikonnen", 100));
                    System.Console.WriteLine(
"Test QBE on class with member re-initialization in constructor");
                    ListResult(result);
                }
                catch (Db4oException ex)
                 {
                    System.Console.WriteLine(
"Db4o Exception: " + ex.Message);
                }
                catch (Exception ex)
                 {
                    System.Console.WriteLine(
"System Exception: " + ex.Message);
                }
                finally
                 {
                    CloseDatabase();
                }
            }
        }</pre>
