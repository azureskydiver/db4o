private static void QueryWithEvaluation(IObjectContainer db)
     {
      IQuery query = db.Query();
      query.Constrain(typeof (Car));
      query.Constrain(new EvenHistoryEvaluation());
      IObjectSet result = query.Execute();
      ListResult(result);
