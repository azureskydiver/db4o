private static IList&lt;Pilot&gt; ByNameBeginning(string startsWith)
         {
            return Database().Query&lt;Pilot&gt;(delegate(Pilot pilot)
             {
                return pilot.Name.StartsWith(startsWith);
            });
        }</pre>    </body></html>