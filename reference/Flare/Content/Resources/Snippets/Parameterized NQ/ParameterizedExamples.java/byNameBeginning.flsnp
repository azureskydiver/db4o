private static List&lt;Pilot&gt; byNameBeginning(final String startsWith)  {
    return database().query(new Predicate&lt;Pilot&gt;()  {
      public boolean match(Pilot pilot)  {
        return pilot.getName().startsWith(startsWith);
      }
    });
