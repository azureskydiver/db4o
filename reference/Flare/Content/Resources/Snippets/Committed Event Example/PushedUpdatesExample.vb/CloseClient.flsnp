1Private Sub CloseClient()Sub CloseClient(ByVal client As IObjectContainer)
2            ' remove listeners before shutting down
3            If Not (clientListeners(client) Is Nothing) Then
4                Dim eventRegistry As IEventRegistry = EventRegistryFactory.ForObjectContainer(client)
5                RemoveHandler eventRegistry.Committed, AddressOf CType(clientListeners(client), CommittedEventHandler).OnCommitted
6                clientListeners.Remove(client)
7            End If
8            client.Close()
9        End Sub</pre>    </body></html>