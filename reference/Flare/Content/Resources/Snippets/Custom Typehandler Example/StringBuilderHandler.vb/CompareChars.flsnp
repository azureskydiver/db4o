1Private Shared Function CompareChars()Function CompareChars(ByVal compare As Char(), ByVal [with] As Char()) As Integer
2            Dim min As Integer = IIf(compare.Length &lt; [with].Length, compare.Length, [with].Length)
3            For i As Integer = 0 To min - 1
4                If compare(i) &lt;&gt; [with](i) Then
5                    Return compare(i).CompareTo([with](i))
6                End If
7            Next
8            Return compare.Length - [with].Length
9        End Function</pre>    </body></html>