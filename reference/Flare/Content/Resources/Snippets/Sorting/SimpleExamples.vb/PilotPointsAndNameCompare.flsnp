1Private Shared Function PilotPointsAndNameCompare()Function PilotPointsAndNameCompare(ByVal p1 As Pilot, ByVal p2 As Pilot) As Integer
2            ' sort by name then by points: descending
3            Dim compareResult As Integer = p1.Name.CompareTo(p2.Name)
4            If (compareResult = 0) Then
5                Return p1.Points - p2.Points
6            Else
7                Return -compareResult
8            End If
9        End Function</pre>    </body></html>