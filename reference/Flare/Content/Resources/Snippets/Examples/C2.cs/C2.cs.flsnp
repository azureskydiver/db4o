using Db4objects.Db4o;

namespace Db4objects.Db4odoc.Constructors
 {
  class C2 
   {
    [Transient] private string _x;
    private string _s;

    private C2(string s) 
     {
      _s=s;
      _x="x";
    }

    override public string ToString() 
     {
      return _s+_x.Length;
    }
  }
}</pre>
