        <pre class="prettyprint lang-vb" xml:space="preserve">Db4oHttpModule.vb: ConfigureAlias
Public Sub ConfigureAlias()
            Dim asm As Assembly = Assembly.GetExecutingAssembly()
            Dim asmName = "Db4objects.Db4odoc.Web.Data.*, " + _ 
asm.GetName().ToString().Substring(0, _ 
asm.GetName().ToString().LastIndexOf("Version") - 2)
            Db4oFactory.Configure().AddAlias(New _ 
WildcardAlias("Db4objects.Db4odoc.Web.Data.*, WebSite", asmName))
        End Sub</pre>
