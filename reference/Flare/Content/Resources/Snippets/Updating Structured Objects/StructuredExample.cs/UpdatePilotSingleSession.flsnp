private static void UpdatePilotSingleSession(IObjectContainer db)
     {
      IObjectSet result = db.Get(new Car(&quot;Ferrari&quot;));
      Car found = (Car)result.Next();
      found.Pilot.AddPoints(1);
      db.Set(found);
      result = db.Get(new Car(&quot;Ferrari&quot;));
      ListResult(result);
    }</pre>    </body></html>