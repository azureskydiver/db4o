private static void UpdateCar(IObjectContainer db)
     {
      IObjectSet result = db.Get(new Car(&quot;Ferrari&quot;));
      Car found = (Car)result.Next();
      found.Pilot = new Pilot(&quot;Somebody else&quot;, 0);
      db.Set(found);
      result = db.Get(new Car(&quot;Ferrari&quot;));
      ListResult(result);
    }</pre>    </body></html>