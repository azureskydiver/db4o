Private Class PilotEvaluationClass PilotEvaluation
            Implements IEvaluation

            Public Sub Evaluate()Sub Evaluate(ByVal candidate As ICandidate) Implements IEvaluation.Evaluate
                Dim pilot As Pilot = CType(candidate.GetObject, Pilot)
                candidate.Include(pilot.Points Mod 2 = 0)
            End Sub
        End Class</pre>    </body></html>