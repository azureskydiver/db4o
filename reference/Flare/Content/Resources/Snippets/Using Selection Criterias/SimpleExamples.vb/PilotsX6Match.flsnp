Private Shared Function PilotsX6Match()Function PilotsX6Match(ByVal p As Pilot) As Boolean
            ' all Pilots with the name ending with 6 will
            ' be included
            Return p.Name.EndsWith(&quot;6&quot;)
        End Function</pre>    </body></html>