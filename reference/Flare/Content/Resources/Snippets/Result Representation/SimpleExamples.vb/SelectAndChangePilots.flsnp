        <pre class="prettyprint" xml:space="preserve">SimpleExamples.vb: SelectAndChangePilots
Private Shared Sub SelectAndChangePilots()
            Dim container As IObjectContainer = Database()
            If Not container Is Nothing Then
                Try
                    Dim result As IList(Of Pilot) = _ 
container.Query(Of Pilot)(AddressOf RankPilotsAndMatch)
                    ListResult(result)
                Catch ex As Exception
                    System.Console.WriteLine("System Exception: " + ex.Message)
                Finally
                    CloseDatabase()
                End Try
            End If
        End Sub</pre>
