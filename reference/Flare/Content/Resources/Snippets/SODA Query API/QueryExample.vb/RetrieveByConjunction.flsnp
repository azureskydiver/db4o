1Public Shared Sub RetrieveByConjunction()Sub RetrieveByConjunction(ByVal db As IObjectContainer)
2            Dim query As IQuery = db.Query()
3            query.Constrain(GetType(Pilot))
4            Dim constr As IConstraint = query.Descend(&quot;_name&quot;).Constrain(&quot;Michael Schumacher&quot;)
5            query.Descend(&quot;_points&quot;).Constrain(99).Or(constr)
6            Dim result As IObjectSet = query.Execute()
7            ListResult(result)
8        End Sub</pre>    </body></html>