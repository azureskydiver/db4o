Private Sub ExecuteSodaQuery(ByVal objectContainer As IObjectContainer)

	Dim Query As IQuery = objectContainer.Query()
	Query.Constrain(GetType(MonitoringDemo.Item))
	Query.Descend("name").Constrain("1")
	Query.Execute()
End Sub
