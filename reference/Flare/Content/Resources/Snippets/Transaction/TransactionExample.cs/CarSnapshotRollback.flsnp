private static void CarSnapshotRollback(IObjectContainer db)
         {
            IObjectSet result = db.Get(new Car(&quot;BMW&quot;));
            Car car = (Car)result.Next();
            car.Snapshot();
            db.Set(car);
            db.Rollback();
            Console.WriteLine(car);
        }</pre>    </body></html>