Public Shared Sub StoreCarRollback()Sub StoreCarRollback(ByVal db As IObjectContainer)
            Dim pilot As Pilot = New Pilot(&quot;Michael Schumacher&quot;, 100)
            Dim car As Car = New Car(&quot;Ferrari&quot;)
            car.Pilot = pilot
            db.Set(car)
            db.Rollback()
        End Sub</pre>    </body></html>