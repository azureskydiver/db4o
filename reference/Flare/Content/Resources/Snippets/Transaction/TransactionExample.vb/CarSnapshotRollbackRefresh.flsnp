Public Shared Sub CarSnapshotRollbackRefresh()Sub CarSnapshotRollbackRefresh(ByVal db As IObjectContainer)
            Dim result As IObjectSet = db.Get(New Car(&quot;BMW&quot;))
            Dim car As Car = DirectCast(result.Next(), Car)
            car.Snapshot()
            db.Set(car)
            db.Rollback()
            db.Ext().Refresh(car, Integer.MaxValue)
            Console.WriteLine(car)
        End Sub</pre>    </body></html>