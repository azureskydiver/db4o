	<div class="codesnippet" MadCap:conditions="Primary.c#">
        <pre class="prettyprint" xml:space="preserve">internal class IndexDiagnostics : IDiagnosticListener
{
    public void OnDiagnostic(IDiagnostic diagnostic)
    {
        if (diagnostic is LoadedFromClassIndex)
        {
            Console.WriteLine(&quot;This query couldn't use field indexes &quot; +
                              ((LoadedFromClassIndex) diagnostic).Reason());
            Console.WriteLine(diagnostic);
        }
    }
}</pre>
