	<div class="codesnippet" MadCap:conditions="Primary.c#">
        <pre class="prettyprint" xml:space="preserve">
PilotGroup <span style="color:Blue;">group</span> = FindGroup(container);
Pilot pilot = <span style="color:Blue;">group</span>.Pilots[0];
<span style="color:Blue;">group</span>.Pilots.Remove(pilot);
container.Store(<span style="color:Blue;">group</span>.Pilots);
container.Delete(pilot);

AssertEquals(2, AllPilots(container).Count);
AssertEquals(2, <span style="color:Blue;">group</span>.Pilots.Count);
</pre>
