<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field">
	<class name="com.db4o.test.replication.template.r0tor4.R0" table="R0">
		<id column="id" type="long">
			<generator class="native"/>
		</id>
		<discriminator/>
		<property name="name"/>
		<many-to-one name="r0" cascade="all"/>
		<many-to-one name="r1" cascade="all"/>
		<subclass name="com.db4o.test.replication.template.r0tor4.R1">
			<many-to-one name="circle1" cascade="all"/>
			<many-to-one name="r2" cascade="all"/>
			<subclass name="com.db4o.test.replication.template.r0tor4.R2">
				<many-to-one name="circle2" cascade="all"/>
				<many-to-one name="r3" cascade="all"/>
				<subclass name="com.db4o.test.replication.template.r0tor4.R3">
					<many-to-one name="circle3" cascade="all"/>
					<many-to-one name="r4" cascade="all"/>
					<subclass name="com.db4o.test.replication.template.r0tor4.R4">
						<many-to-one name="circle4" cascade="all"/>
					</subclass>
				</subclass>
			</subclass>
		</subclass>
	</class>
</hibernate-mapping>
